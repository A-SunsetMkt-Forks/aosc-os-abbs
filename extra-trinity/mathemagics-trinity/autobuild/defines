PKGNAME=mathemagics-trinity
PKGSEC=Trinity
PKGDEP="tdelibs"
PKGDES="An RPN (Reverse Polish Notation) calculator for Trinity Desktop Environment"

CMAKE_AFTER="-DLIB_INSTALL_DIR=/usr/lib \
             -DWITH_ALL_OPTIONS=ON \
             -DWITH_GCC_VISIBILITY=OFF \
             -DBUILD_ALL=ON"

PKGCONFL="mathemagics"
