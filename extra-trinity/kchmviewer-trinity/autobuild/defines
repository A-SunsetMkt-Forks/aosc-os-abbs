PKGNAME=kchmviewer-trinity
PKGSEC=Trinity
PKGDEP="chmlib tdelibs"
PKGDES="A CHM (Compiled HTML Help) file viewer for Trinity Desktop Environment"

CMAKE_AFTER="-DLIB_INSTALL_DIR=/usr/lib \
             -DWITH_ALL_OPTIONS=ON \
             -DWITH_GCC_VISIBILITY=OFF \
             -DBUILD_ALL=ON"

PKGCONFL="kchmviewer"

NOLIBTOOL=0
