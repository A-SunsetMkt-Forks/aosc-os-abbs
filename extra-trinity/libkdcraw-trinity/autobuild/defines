PKGNAME=libkdcraw-trinity
PKGSEC=Trinity
PKGDEP="libraw tdelibs tqtinterface"
PKGDES="A libkdcraw library interface for Trinity Desktop Environment"

AUTOTOOLS_AFTER="--enable-final \
                 --enable-new-ldflags \
                 --enable-pie \
                 --disable-rpath \
                 --disable-gcc-hidden-visibility"

PKGBREAK="trinity-libkdcraw"
PKGREP="trinity-libkdcraw"
PKGCONFL="libkdcraw"
