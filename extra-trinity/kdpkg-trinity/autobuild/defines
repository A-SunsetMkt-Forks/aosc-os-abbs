PKGNAME=kdpkg-trinity
PKGSEC=Trinity
PKGDEP="dpkg tdelibs"
PKGDES="A DPKG (.deb) package installer GUI for Trinity Desktop Environment"

CMAKE_AFTER="-DLIB_INSTALL_DIR=/usr/lib \
             -DWITH_ALL_OPTIONS=ON \
             -DWITH_GCC_VISIBILITY=OFF \
             -DBUILD_ALL=ON"

PKGCONFL="kdpkg"

NOLIBTOOL=0
