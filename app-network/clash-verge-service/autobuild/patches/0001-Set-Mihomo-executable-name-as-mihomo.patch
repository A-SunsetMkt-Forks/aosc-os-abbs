From 423a6345521e345550f403fee7f3afa1184d4247 Mon Sep 17 00:00:00 2001
From: Mingcong Bai <jeffbai@aosc.xyz>
Date: Mon, 8 Jul 2024 12:02:03 +0800
Subject: [PATCH] Set Mihomo executable name as mihomo

Co-authored-by: eatradish <sakiiily@aosc.io>
---
 src/service/web.rs | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/src/service/web.rs b/src/service/web.rs
index 3492be7..44c61bb 100644
--- a/src/service/web.rs
+++ b/src/service/web.rs
@@ -80,7 +80,7 @@ pub fn stop_clash() -> Result<()> {
     let system = System::new_with_specifics(
         RefreshKind::new().with_processes(ProcessRefreshKind::everything()),
     );
-    let procs = system.processes_by_name("verge-mihomo");
+    let procs = system.processes_by_name("mihomo");
     for proc in procs {
         proc.kill();
     }
