abinfo "Building PySide2 ..."
python3 "$SRCDIR"/setup.py build \
    --disable-numpy-support ||
    abdie "Failed to build Python (PyPI) package using ${PYTHON}: $?."
BUILD_FINAL

abinfo "Installing PySide2 ..."
python3 "$SRCDIR"/setup.py install \
    --prefix=/usr --root="$PKGDIR" --optimize=1 ||
    abdie "Failed to install Python (PyPI) package using ${PYTHON}: $?."
